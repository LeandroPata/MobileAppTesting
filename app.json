{
  "expo": {
    "name": "AppTesting",
    "slug": "AppTesting",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/P5logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/P5logo.png",
      "resizeMode": "contain",
